

<form method="post" >
  <%= token_tag(nil) %>
  <fieldset>
    <legend>Sign In</legend>
    <label>Email</label>
    <input type="text" value="<%= @email %>" placeholder="Enter User name here..." name="email">
    <!-- <span class="help-block">Example block-level help text here.</span> -->
    <!-- <label class="checkbox"><input type="checkbox"> Check me out</label>-->

    <label>Password</label>
    <input type="password" placeholder="Enter Password here..." name="password" >
    
    </br>
    <button type="submit" class="btn">Log In</button>
  </fieldset>
  <br><br>
  <p><a href="/auth/facebook">Log In With Facebook</a></p>

</form>


