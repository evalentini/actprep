

<div class="row-fluid" id="alert-row">
	<div class="span1"></div>
	<div class="span10">
		<div class="row-fluid" style="border-bottom: 1px dashed black; margin-bottom: 25px;">
			<div class="span12">
				<h4>List of Questions</h4>
			</div>
		</div>
		<div class="row-fluid">
			<div class="span12">
				<table class="table table-bordered">
					<tr><td>Test #</td><td>Section</td><td>Question #</td></tr>
					<% @questions.each do |question| %>
					<tr>
						<td><%= question.test_number %></td>
						<td><%= question.section %></td>
						<td><%= question.question_number%></td>
					</tr>
					<% end %>
				</table>
			</div>
		</div>
		
		<div class="row-fluid" style="border-bottom: 1px dashed black; margin-bottom: 25px;">
			<div class="span12">
				<h4>Add New Question</h4>
			</div>
		</div>
		
		<div class="row-fluid">
		</div>
				
		<div class="row-fluid">
			<div class="span12">
				<button class="btn btn-primary">Add Question</button>
			</div>
		</div>
		

	</div>
	<div class="span1"></div>
</div>

